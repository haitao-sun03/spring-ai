{
  "mcpServers": {
    "baidu-map": {
      "command": "npx.cmd",
      "args": [
        "-y",
        "@baidumap/mcp-server-baidu-map"
      ],
      "env": {
        "BAIDU_MAP_API_KEY": "xxx"
      }
    },
    "search-picture-mcp-server": {
      "command": "java",
      "args": [
        "-Dspring.ai.mcp.server.stdio=true",
        "-Dspring.main.web-application-type=none",
        "-Dlogging.pattern.console=",
        "-jar",
        "search-pictures-mcp-server/search-pictures-mcp-server/target/search-pictures-mcp-server-0.0.1-SNAPSHOT.jar"
      ],
      "env": {}
    }
}
}